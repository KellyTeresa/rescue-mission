<div class="row">
  <div class="large-12">

    <h1>Submit a New Question</h1>

    <%= form_for @question do |f| %>
      <%= f.label :title %>
      <%= f.text_field :title %>

      <%= f.label :description %>
      <%= f.text_area :description %>

      <%= f.submit "Submit Question"%>
    <%- end -%>

  </div>
</div>
